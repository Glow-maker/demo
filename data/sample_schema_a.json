{
  "database": "source_database",
  "table": "products",
  "columns": [
    {
      "name": "product_id",
      "type": "INTEGER",
      "description": "Unique product identifier"
    },
    {
      "name": "product_name",
      "type": "VARCHAR(200)",
      "description": "Name of the product"
    },
    {
      "name": "price",
      "type": "DECIMAL(10,2)",
      "description": "Product price in USD"
    },
    {
      "name": "stock_quantity",
      "type": "INTEGER",
      "description": "Available inventory quantity"
    },
    {
      "name": "category_id",
      "type": "INTEGER",
      "description": "Product category identifier"
    },
    {
      "name": "manufacturer",
      "type": "VARCHAR(100)",
      "description": "Manufacturer or brand name"
    },
    {
      "name": "created_date",
      "type": "DATETIME",
      "description": "Date when product was added to system"
    }
  ]
}
